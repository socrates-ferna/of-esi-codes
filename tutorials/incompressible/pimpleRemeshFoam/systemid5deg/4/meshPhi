FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "4";
    object      meshPhi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 3 -1 0 0 0 0];
oriented        oriented;


internalField   uniform 0;

boundaryField
{
    airfoil
    {
        type            calculated;
        value           uniform 0;
    }
    freestream
    {
        type            calculated;
        value           uniform 0;
    }
    BaseAndTop
    {
        type            empty;
        value           nonuniform List<scalar> 0();
    }
}


