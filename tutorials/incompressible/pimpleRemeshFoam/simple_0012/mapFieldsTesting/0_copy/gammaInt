FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      gammaInt;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];


internalField   uniform 1;

boundaryField
{
    airfoil
    {
        type            zeroGradient;
    }
    freestream
    {
        type            freestream;
        freestreamValue uniform 1;
        value           uniform 1;
    }
    BaseAndTop
    {
        type            empty;
    }
}


