FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];


internalField   uniform 0.0002;

boundaryField
{
    flap
    {
        type            fixedValue;
        value           uniform 5e-07;
    }
    freestream
    {
        type            fixedValue;
        value           uniform 0.0002;
    }
    main
    {
        type            fixedValue;
        value           nonuniform List<scalar> 0();
    }
    BaseAndTop
    {
        type            empty;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 0.0002;
    }
}


