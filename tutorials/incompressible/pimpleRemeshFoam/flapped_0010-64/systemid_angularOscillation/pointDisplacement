FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    location    "0";
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    out
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    sym1
    {
        type            symmetryPlane;
    }
    sym2
    {
        type            symmetryPlane;
    }
    in
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    cylinder
    {
        type            oscillatingDisplacement;
        amplitude       ( 0 4 0 );
        omega           6.28318;
        value           uniform ( 0 0 0 );
    }
    back
    {
        type            empty;
    }
    front
    {
        type            empty;
    }
}


